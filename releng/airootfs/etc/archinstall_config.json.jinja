{
  "additional-repositories": [],
  "archinstall-language": "English",
  "audio_config": "pipewire",
  "bootloader": "grub",
  "debug": false,
  {#- disk_config -#}
  {#- disk_encryption #}
  "hostname": "{{ hostname }}",
  "kernels": [
    "linux"
  ],
  "locale_config": {
    "kb_layout": "es",
    "sys_enc": "UTF-8",
    "sys_lang": "en_US"
  },
  "network_config": {},
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "packages": [
    {%- for p in packages %}
    "{{ p }}"{{ "," if not loop.last }}
    {%- endfor %}
  ],
  "parallel downloads": 0,
  "profile_config": null,
  "save_config": null,
  "script": "guided",
  "silent": false,
  "swap": true,
  "timezone": "UTC",
  "version": "2.6.0"
}
